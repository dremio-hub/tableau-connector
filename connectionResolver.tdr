<?xml version='1.0' encoding='utf-8' ?>
<tdr class='dremio'>
  <connection-resolver>
    <connection-builder>
      <script file='connectionBuilder.js'/>
    </connection-builder>
    <connection-normalizer>
      <required-attributes>
        <setImpersonateAttributes/>
        <attribute-list>
          <attr>class</attr>
          <attr>server</attr>
          <attr>port</attr>
          <attr>username</attr>
          <attr>password</attr>
          <attr>sslmode</attr>
        </attribute-list>
      </required-attributes>
    </connection-normalizer>
  </connection-resolver>
</tdr>
