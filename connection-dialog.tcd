<connection-dialog class='dremio'>
  <connection-config>
    <authentication-mode value='Basic' />
    <authentication-options>
      <option name="UsernameAndPassword" default="true" />
    </authentication-options>
    <has-databases value="true" />
    <has-schemas value="true" />
    <port-prompt value="Port: " default="31010" />
    <show-db-browse-button value="true" />
    <show-ssl-checkbox value="true" />
  </connection-config>
</connection-dialog>
